package com.biaozhunyuan.tianyi.common.model.request;

import java.io.Serializable;

/**
 * 字典项
 *
 * @author K
 */
public class Dict implements Serializable {
    /**
     *
     */
    private static final long serialVersionUID = 2448497598284609694L;
    int _Id;

    public int 编号;

    public String 名称;

    public String 拼音;

    public int 上级字典;

    public Dict() {
        super();
    }

    public Dict(int 编号, String 名称) {
        super();
        this.编号 = 编号;
        this.名称 = 名称;
    }

}
