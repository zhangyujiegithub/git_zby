<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.tianyi.project.FormSelectPurchaseActivity">

    <com.biaozhunyuan.tianyi.common.view.BoeryunHeaderView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/headerview"
        app:titleText="采购明细"
        app:titleRightText="确定"
        app:isVisbleRightText="true"
        app:isVisbleBackIco="true"></com.biaozhunyuan.tianyi.common.view.BoeryunHeaderView>

    <ListView
        android:id="@+id/lv"
        android:layout_width="match_parent"
        android:divider="@drawable/list_divider"
        android:dividerHeight="0.5dp"
        android:layout_height="match_parent"></ListView>

</LinearLayout>
