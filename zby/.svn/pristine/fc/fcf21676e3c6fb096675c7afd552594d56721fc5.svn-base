package com.biaozhunyuan.tianyi.space;

import java.io.Serializable;

/**
 * 空间实体类
 */

public class Space implements Serializable {
    private String uuid; //发帖人id
    private String status;
    private String createTime; //发布时间
    private String createrName; //发布人名字
    private String postType;  //帖子类型
    private String postTypeChild; //帖子子类型
    private String postTitle; //帖子标题
    private String textPart;  //帖子内容
    private String picAttachmentIds; //图片id
    private String fileAttachmentIds;
    private String name; //@用户名
    private String copyToStaffs; //被@人的uuid
    private String creatorId; // 发布人头像
    private String recipientIds;
    private String reciveCategory;
    private String postTypeName; //空间类型名称

    public String getPostTypeName() {
        return postTypeName;
    }

    public void setPostTypeName(String postTypeName) {
        this.postTypeName = postTypeName;
    }

    public String getCreaterName() {
        return createrName;
    }

    public void setCreaterName(String createrName) {
        this.createrName = createrName;
    }

    public String getRecipientIds() {
        return recipientIds;
    }

    public void setRecipientIds(String recipientIds) {
        this.recipientIds = recipientIds;
    }

    public String getReciveCategory() {
        return reciveCategory;
    }

    public void setReciveCategory(String reciveCategory) {
        this.reciveCategory = reciveCategory;
    }

    public String getCreatorId() {
        return creatorId;
    }

    public void setCreatorId(String creatorId) {
        this.creatorId = creatorId;
    }

    public String getCreateName() {
        return createrName;
    }

    public void setCreateName(String createName) {
        this.createrName = createName;
    }

    private boolean isLike; //是否已经点赞
    private boolean isFavorite; //是否收藏
    private int commentNumber; //评论数
    private int likeNumber; // 点赞数
    private int favoriteNumber; //收藏数

    public boolean isLike() {
        return isLike;
    }

    public void setLike(boolean like) {
        isLike = like;
    }

    public boolean isFavorite() {
        return isFavorite;
    }

    public void setFavorite(boolean favorite) {
        isFavorite = favorite;
    }

    public int getCommentNumber() {
        return commentNumber;
    }

    public void setCommentNumber(int commentNumber) {
        this.commentNumber = commentNumber;
    }

    public int getLikeNumber() {
        return likeNumber;
    }

    public void setLikeNumber(int likeNumber) {
        this.likeNumber = likeNumber;
    }

    public int getFavoriteNumber() {
        return favoriteNumber;
    }

    public void setFavoriteNumber(int favoriteNumber) {
        this.favoriteNumber = favoriteNumber;
    }

    public String getUuid() {
        return uuid;
    }

    public void setUuid(String uuid) {
        this.uuid = uuid;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getCreateTime() {
        return createTime;
    }

    public void setCreateTime(String createTime) {
        this.createTime = createTime;
    }

    public String getPostType() {
        return postType;
    }

    public void setPostType(String postType) {
        this.postType = postType;
    }

    public String getPostTypeChild() {
        return postTypeChild;
    }

    public void setPostTypeChild(String postTypeChild) {
        this.postTypeChild = postTypeChild;
    }

    public String getPostTitle() {
        return postTitle;
    }

    public void setPostTitle(String postTitle) {
        this.postTitle = postTitle;
    }

    public String getTextPart() {
        return textPart;
    }

    public void setTextPart(String textPart) {
        this.textPart = textPart;
    }

    public String getPicAttachmentIds() {
        return picAttachmentIds;
    }

    public void setPicAttachmentIds(String picAttachmentIds) {
        this.picAttachmentIds = picAttachmentIds;
    }

    public String getFileAttachmentIds() {
        return fileAttachmentIds;
    }

    public void setFileAttachmentIds(String fileAttachmentIds) {
        this.fileAttachmentIds = fileAttachmentIds;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getCopyToStaffs() {
        return copyToStaffs;
    }

    public void setCopyToStaffs(String copyToStaffs) {
        this.copyToStaffs = copyToStaffs;
    }
}
