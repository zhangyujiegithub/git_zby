package com.biaozhunyuan.tianyi.task;

/**
 * Created by 王安民 on 2017/8/24.
 * 任务状态的枚举类
 */

public enum TaskStatusEnum {

    已完成("372f87e0838e40359fb3e82bce8c1f0b", "已完成"), 进行中("a44ba1d90df44381870421cc46d2983a", "进行中"),
    已取消("cbe01d5eb84b4bd4b95b72ee4167ccfd", "已取消"), 已逾期("cc662530f7ce458abc951b5e3568a823", "已逾期");

    // 私有成员变量，保存名称
    private String name;
    private String status;



    public String getName() {
        return name;
    }

    public String getStatus() {
        return status;
    }

    // 带参构造函数
    TaskStatusEnum(String name, String status) {
        this.name = name;
        this.status = status;
    }
}
