package com.biaozhunyuan.tianyi.buglist;

import java.io.Serializable;

/**
 * Bug相关用到的实体类
 */

public class Bug implements Serializable{
    private String content; //bug内容
    private String title; //bug内容
    private String attachmentIds; //附件
    private String currentDesignateId; //当前指派人
    private String projectManagementName; //项目名称
    private String projectManagement; //项目id
    private String createTime; //创建时间
    private String projectMange; //指派人
    private String creatorId; //创建人
    private String status; //当前状态
    private String statusName; //当前状态
    private String uuid;

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getUuid() {
        return uuid;
    }

    public void setUuid(String uuid) {
        this.uuid = uuid;
    }

    public String getProjectManagementName() {
        return projectManagementName;
    }

    public void setProjectManagementName(String projectManagementName) {
        this.projectManagementName = projectManagementName;
    }

    public String getCreateTime() {
        return createTime;
    }

    public void setCreateTime(String createTime) {
        this.createTime = createTime;
    }

    public String getProjectMange() {
        return projectMange;
    }

    public void setProjectMange(String projectMange) {
        this.projectMange = projectMange;
    }

    public String getCreatorId() {
        return creatorId;
    }

    public void setCreatorId(String creatorId) {
        this.creatorId = creatorId;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getStatusName() {
        return statusName;
    }

    public void setStatusName(String statusName) {
        this.statusName = statusName;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public String getAttachmentIds() {
        return attachmentIds;
    }

    public void setAttachmentIds(String attachmentIds) {
        this.attachmentIds = attachmentIds;
    }

    public String getCurrentDesignateId() {
        return currentDesignateId;
    }

    public void setCurrentDesignateId(String currentDesignateId) {
        this.currentDesignateId = currentDesignateId;
    }

    public String getProjectManagement() {
        return projectManagement;
    }

    public void setProjectManagement(String projectManagement) {
        this.projectManagement = projectManagement;
    }
}
